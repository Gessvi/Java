import java.util.Scanner;
public class main {
static Scanner leia = new Scanner (System.in);
	
	public static void main(String[] args) {	
		int numero = 20;
		
		while (numero != 10) {
			
			System.out.printf("Insira um numero:");
			numero = leia.nextInt();
			
			if (numero == 10) 
				System.out.printf("Você Acertou!");
				else 
					System.out.printf("Você Errou! \n");
		}
		
		
		leia.close();
	}

}
