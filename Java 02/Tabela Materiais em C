#include <stdio.h>

int main()
{
    int codigo1, codigo2, codigo3;

    printf ("O material é importante para a empresa?" "\n [1] - SIM" "\n [2] - NÃO" "\n");
    scanf ("%i", &codigo1);
    printf ("É de importancia para a equipe" "\n [1] - SIM" "\n [2] - NÃO" "\n");
    scanf ("%i", &codigo2);
    printf ("É similar? " "\n [1] - SIM" "\n [2] - NÃO" "\n");
    scanf ("%i", &codigo3);
    
  if (codigo1 == 1 && codigo2 == 1 && codigo3 == 1)
  {
      printf(" [ Y ] - Material de importancia mediana, com possibilidade de similar na empresa");
      
  }
  else if (codigo1 == 1 && codigo2 == 1 && codigo3 == 2)
  {
      printf("[ Z ] - Material de importancia vital, sem possibilidade de similar existente na empresa");
  }
  else
  {
      printf("[ X ] - Material sem importancia, com possibilidade de similar existente na empresa");
      
  }
  
    return 0;
}
